generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id           Int       @id @default(autoincrement())
  code         String    @unique
  targetUrl    String
  createdAt    DateTime  @default(now())
  deleted      Boolean   @default(false)
  totalClicks  Int       @default(0)
  lastClicked  DateTime?
}
